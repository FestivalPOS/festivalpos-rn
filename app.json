{
  "expo": {
    "name": "FestivalPOS",
    "slug": "festivalpos-rn",
    "scheme": "festival-pos",
    "owner": "festival-pos",
    "version": "1.0.2",
    "githubUrl": "https://github.com/FestivalPOS/festivalpos-rn",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#151718"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sg.aufgetischt.festival-pos",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to use the camera to scan a QR code",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to use the microphone."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A283C"
      },
      "package": "sg.aufgetischt.festival_pos",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "7f60a1bf-03cb-45a1-8b66-9acb1db3a305"
      }
    }
  }
}
